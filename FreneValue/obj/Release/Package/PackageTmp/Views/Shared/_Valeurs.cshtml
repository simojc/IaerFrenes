@model IEnumerable<FreneValue.Models.VAL_DOM>

@*<div class="ValeursDom">*@

   
    <table class="table table-bordered table-striped" id="RstRecherche">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.COD_DOM)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.COD_VAL)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VAL)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ACTIF)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DESCRIP)
            </th>
         
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.COD_DOM)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.COD_VAL)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.VAL)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ACTIF)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DESCRIP)
                </td>
                <td>
                    <div class="pull-right">

                        @Html.ActionLink(" ", "Edit", new { id = item.ID }, new { @class = "glyphicon glyphicon-pencil", title = "Modifier" })
                        &nbsp;
                        &nbsp;
                        @Html.ActionLink(" ", "Delete", new { id = item.ID }, new { @class = "glyphicon glyphicon-trash", title = "Supprimer" })
                    </div>
                </td>

            </tr>
        }

    </table>
@*</div>*@

